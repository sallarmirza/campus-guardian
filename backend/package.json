{
  "name": "campus-guard-backend",
  "version": "1.0.0",
  "description": "Backend API server for Campus Guardian surveillance system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --watchAll=false",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "create:migration": "sequelize-cli migration:generate --name",
    "create:seed": "sequelize-cli seed:generate --name",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1467.0",
    "socket.io": "^4.7.2",
    "joi": "^17.9.2",
    "morgan": "^1.10.0",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "nodemailer": "^6.9.4",
    "uuid": "^9.0.0",
    "sharp": "^0.32.5",
    "express-fileupload": "^1.4.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "axios": "^1.5.0",
    "winston": "^3.10.0",
    "express-winston": "^4.2.0",
    "cron": "^2.4.4",
    "csv-parser": "^3.0.0",
    "excel4node": "^1.8.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.4",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.0.2",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1"
  },
  "keywords": ["surveillance", "campus", "security", "api", "nodejs", "express"],
  "author": "Campus Guardian Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}